{
  "name": "@bkl/monorepo",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.6",
  "notInUse": {
    "test": "cd frontend/brooklynlearning & vitest",
    "test:frontend": "pnpm -C packages/frontend/brooklynlearning vitest",
    "test:unit": "pnpm -C packages/frontend/brooklynlearning run test:unit",
    "test:e2e": "pnpm -C packages/frontend/brooklynlearning run test:e2e"
  },
  "scripts": {
    "test:frontend": "pnpm -C ./frontend/brooklynlearning run test",
    "dev:clippy": "pnpm -C ./apps/examples/ClipboardCollector run dev",
    "build:clippy": "pnpm -C ./apps/examples/ClipboardCollector run build",
    "dev:repl": "pnpm -C ./apps/examples/Repl run dev"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "devDependencies": {
    "cypress": "^13.7.2",
    "jsdom": "^24.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  }
}
